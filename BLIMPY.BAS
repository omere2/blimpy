SUB DEATHEFFECT (X, Y)
FOR I = 1 TO 10 STEP 4
  CIRCLE (X + 10, Y + 10), I, 14
  FOR A = 1 TO 500: NEXT
  CIRCLE (X + 10, Y + 10), I + 1, 12
  FOR A = 1 TO 500: NEXT
  CIRCLE (X + 10, Y + 10), I + 2, 13
  FOR A = 1 TO 500: NEXT
  CIRCLE (X + 10, Y + 10), I + 3, 4
  FOR A = 1 TO 500: NEXT
NEXT
END SUB

SUB DEATHSUB
DIM A(27, 27)
CIRCLE (15, 15), 12, 15
PAINT (15, 15), 15, 15
LINE (6, 8)-(10, 12), 9
LINE (6, 12)-(10, 8), 9
LINE (13, 8)-(17, 12), 9
LINE (13, 12)-(17, 8), 9
LINE (20, 8)-(24, 12), 9
LINE (20, 12)-(24, 8), 9
CIRCLE (15, 20), 3, 4, 0!, 3.3
FOR X = 0 TO 27
FOR Y = 0 TO 27
A(X, Y) = POINT(X, Y)
NEXT
NEXT
LINE (0, 0)-(27, 27), 0, BF
FOR X = 1 TO 28
FOR Y = 1 TO 28
LINE (X * 18 + 18, Y * 18 - 36)-(X * 18 + 36, Y * 18 - 18), A(X - 1, Y - 1), BF
NEXT
NEXT
SLEEP (0)
END SUB

