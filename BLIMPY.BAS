        IF POINT(X + 20, Y + 21) = 5 AND UNJUMP > 0 THEN UNJUMP = 0
       
        IF POINT(X, Y + 21) = 4 AND UNJUMP > 0 THEN UNJUMP = 0
        IF POINT(X + 20, Y + 21) = 4 AND UNJUMP > 0 THEN UNJUMP = 0
     
